<struts>
	<package name="conf" namespace="/"	extends="struts-base">
		<action name="conf_*" method="{1}" class="confAction">
			<result name="listReserve">/confmanage/listReserve.jsp</result>
			<result name="reserveConf">/confmanage/reserveConf.jsp</result>
			<result name="modifyReserve">/confmanage/modifyReserve.jsp</result>
